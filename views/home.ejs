        <div id="wrapper">
            <div class="section landing-page text-center active">
                <h1 style="font-size: 60px;"><span style="font-size: 90px;">&alpha;</span>Bit</h1>
                <a href="roadmap" class="btn btn-info" >RoadMap for CSE</a>
            </div>
            <div class="section ">
                <div class="text-center m-3" style="font-size:25px; color: white">Events</div>
                <div class="row">
                    <% Events.forEach(function(event) {
                             %>
                            <div class="card" class="ml-3 "; style="width:40%; margin-left: 5rem; margin-top: 1rem;">
                                  <div class="card-body">
                                  <h5 class="card-title text-capitalize"><strong>
                                  <%= event.title%></strong>
                                  <span class="float-right btn btn-dark btn-sm">
                                   <%= event.EventType%>
                                  </span>
                                  </h5>
                                  <h6 class="card-subtitle mb-2 text-muted">
                                  <%= event.startTime%> - <%= event.endTime%>

                                  </h6>
                                  <p class="card-text">
                                      <% var EventDesc = event.breif ;
                                      try{
                                          var EventDesc =EventDesc.substr(0,120) ;
                                      }catch(e){

                                      }%>
                                      <%= EventDesc  %>
                                  </p>
                                  <a href="/events/<%= event._id %>" class="card-link btn btn-info">View</a>
                                  <a href="#" class="btn btn-dark"><i class="fa fa-hand-o-up"></i> <%= event.interested.length %> </a>

                                  <span class="float-right">
                                  By <%= event.author.username%>
                                  </span>
                                  </div>
                            </div>
                              <% });%>
                        </div>

                <div class="m-3 text-center">
                    <a href="events" class="btn btn-success ">View More Events</a>
                    <a href="events/new" class="btn btn-success">Submit an Event</a>
                </div>
            </div>
            <div class="section ">
