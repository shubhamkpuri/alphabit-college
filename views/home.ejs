        <div id="wrapper">
            <div class="section landing-page text-center ">
                <h1 style="font-size: 6rem;"><span style="font-size: 9rem;">&alpha;</span>Bit</h1>
                <a href="roadmap" class="btn btn-info btn-lg">RoadMap for CSE</a>
            </div>
            <div class="section ">
                <div class="container">
                    <h1>Events</h1>
                    <div class="row mt-3">
                        <% Events.forEach(function(event) { %>
                            <div class="col-lg-5 col-sm-10 mt-3">
                                <div class="card">
                                    <div class="card-body">
                                        <h5 class="card-title  text-capitalize"><strong><%= event.title%></strong>
                                      <span class="float-right btn btn-dark btn-sm"><%= event.EventType%></span>
                                        </h5>
                                        <h6 class="card-subtitle mb-2 text-muted">
                                    <span class="float-left"> Timings :</span>
                                    <br>
                                    <div class="float-left bg-primary text-white p-1 rounded text-center mx-2">
                                         <strong><%= event.startTime.slice(0,10) %>
                                         <br><%= event.startTime.slice(11,16) %></strong>
                                     </div>
                                     <div class="float-left bg-primary text-white p-1 rounded text-center mx-2">
                                          <strong><%= event.endTime.slice(0,10) %>
                                          <br><%= event.endTime.slice(11,16) %></strong>
                                      </div>
                                      <br>
                                      <br>
                              </h6>
                                        <p class="card-text">
                                            <% var EventDesc = event.breif ;
                              try{
                                  var EventDesc =EventDesc.substr(0,120) ;
                              }catch(e){

                              }%>
                                                <%= EventDesc  %>

                                        </p>
                                        <a href="/events/<%= event._id %>" class="card-link btn btn-info">View</a>
                                        <a href="#" class="btn btn-dark"><i class="fa fa-hand-o-up"></i>  <%= event.interested.length %> </a>

                                        <span class="float-right">
                              By <%= event.author.username%>
                              </span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-1 col-sm-1"></div>
                            <% });%>

                    </div>
                    <div class="m-3 text-center">
                        <a href="events" class="btn btn-success m-1">View More Events</a>
                        <a href="events/new" class="btn btn-success m-1">Submit an Event</a>
                    </div>
                </div>
            </div>
            <div class="section ">
                <div class="container">
                    <h1>Projects</h1>

                    <div class="row ">
                        <% Projects.forEach(function(project) { %>

                            <div class="col-lg-5 col-sm-10 mt-3">
